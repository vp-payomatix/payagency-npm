{
  "name": "payagency-api-beta",
  "version": "1.0.11",
  "description": "Reusable API client with TypeScript (CommonJS)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && npm run copy-types",
    "copy-types": "copyfiles -u 1 src/index.cjs.d.ts dist",
    "prepare": "npm run build",
    "link": "npm link && npm link payagency-api-beta",
    "test": "jest"
  },
  "author": "PaneruVipin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vp-payomatix/payagency-npm.git"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "copyfiles": "^2.4.1",
    "dotenv": "^17.2.3",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "axios": "^1.12.2"
  },
  "exports": {
    ".": {
      "import": "./disti/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "index": [
        "dist/index.d.ts"
      ],
      "index.cjs": [
        "dist/index.cjs.d.ts"
      ]
    }
  }
}
